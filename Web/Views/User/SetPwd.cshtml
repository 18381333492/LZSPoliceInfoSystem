
@{
    Layout = null;
}
<style>
    #dataForm p{
        text-align:center
    }
</style>
<!--修改密码-->
<form id="dataForm" style="margin-top:30px;">
    <input type="hidden" name="ID" value="@ViewBag.ID" />
    <div style="padding-left:5px;margin:auto">
        <p>
            <label>新密码：</label><input id="pwd" class="easyui-textbox" name="sNewPwd" data-options="required:true,validType:'length[6,255]'"
                                      invalidmessage="密码长度应大于6位" type="password">
        </p>
    </div>
</form>